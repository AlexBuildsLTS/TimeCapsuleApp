{
  "expo": {
    "name": "northtime-capsule-app",
    "slug": "timecapsule-app",
    "owner": "alexbuildslts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "html": {
        "template": "./index.html"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ca2a982b-3943-4e91-a00d-1093c3d43c83"
      }
    },
    "android": {
      "package": "com.northstarproject.timecapsule"
    }
  }
}
